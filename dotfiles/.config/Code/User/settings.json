    "breadcrumbs.enabled": true,
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c11",
    "code-runner.executorMap": {
        "c": "cd $dir && gcc \"$fileName\" -o \"$fileNameWithoutExt\" && \"./$fileNameWithoutExt\"",
        "python": "python3"
    },
    "editor.accessibilitySupport": "off",
    "editor.rulers": [80],
