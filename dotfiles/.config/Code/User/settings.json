        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "bibtex8",
            "command": "bibtex8",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "dvipng",
            "command": "dvipng",
            "args": [
                "-T tight",
                "-D 120",
                "%DOCFILE%.dvi"
            ],
            "env": {}
        },
        {
            "name": "dvipdf",
            "command": "dvipdf",
            "args": [
                "%DOCFILE%.dvi"
            ],
            "env": {}
        },
        {
            "name": "dvips",
            "command": "dvips",
            "args": [
                "%DOCFILE%.dvi"
            ]
        },
        {
            "name": "latex",
            "command": "latex",
            "args": [
                "-interaction=nonstopmode",
                "-outdir=%OUTDIR%",
                "-shell-escape",
                "-src",
                "%DOC%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "latexmk-clean",
            "command": "latexmk",
            "args": [
                "-outdir=%OUTDIR%",
                "-C"
            ]
        },
        {
            "name": "latexmk-xelatex",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-file-line-error",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "makeindex",
            "command": "makeindex",
            "args": [
                "%DOCFILE%.idx"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-file-line-error",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "ps2pdf",
            "command": "ps2pdf",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-file-line-error",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-synctex=1",
                "%DOC%"
              ]
        },
    ],
